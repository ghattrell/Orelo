= form_for @call do |f|
  - if @call.errors.any?
    #error_explanation
      %h2= "#{pluralize(@call.errors.count, "error")} prohibited this call from being saved:"
      %ul
        - @call.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :user2_id
    = f.number_field :user2_id
  .field
    = f.label :room
    = f.text_field :room
  .field
    = f.label :create
    = f.text_field :create
  .field
    = f.label :destroy
    = f.text_field :destroy
  .actions
    = f.submit 'Save'
